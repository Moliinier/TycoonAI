function TycoonAI_Ultimate:Teach(concept: string, information: any)
    print("\nğŸ“ Aprendiendo nuevo concepto...")
    print("   ğŸ“– Concepto: " .. concept)
    
    local existing = nil
    for i, learned in ipairs(self.persistent_memory.learned_concepts) do
        if learned.concept:lower() == concept:lower() then
            existing = i
            break
        end
    end
    
    local new_learning = {
        concept = concept,
        information = information,
        learned_at = os.date(),
        times_used = 0,
        confidence = 1.0,
        type = type(information)
    }
    
    if existing then
        self.persistent_memory.learned_concepts[existing] = new_learning
        print("   âœ… Concepto actualizado!")
    else
        table.insert(self.persistent_memory.learned_concepts, new_learning)
        self.persistent_memory.stats.things_learned = self.persistent_memory.stats.things_learned + 1
        print("   âœ… Nuevo concepto aprendido!")
    end
    
    if self.learning_system.auto_save then
        self:SaveMemory()
    end
    
    return true
end

function TycoonAI_Ultimate:Recall(concept: string): any?
    for _, learned in ipairs(self.persistent_memory.learned_concepts) do
        if learned.concept:lower():find(concept:lower()) or 
           concept:lower():find(learned.concept:lower()) then
            learned.times_used = learned.times_used + 1
            print("ğŸ’¡ Recuerdo: " .. learned.concept)
            return learned.information
        end
    end
    return nil
end

