local SaveSystem = {}
local SAVE_FOLDER = "TycoonAI_Memory"
local SAVE_FILE = SAVE_FOLDER .. "/my_ai_memory.json"

function SaveSystem.SaveToFile(data: {[string]: any})
    local success, result = pcall(function()
        if writefile then
            if not isfolder(SAVE_FOLDER) then
                makefolder(SAVE_FOLDER)
            end
            local jsonData = HttpService:JSONEncode(data)
            writefile(SAVE_FILE, jsonData)
            return true
        end
        if syn and syn.write_file then
            local jsonData = HttpService:JSONEncode(data)
            syn.write_file(SAVE_FILE, jsonData)
            return true
        end
        return false
    end)
    
    if success and result then
        print("ğŸ’¾ Memoria guardada en: " .. SAVE_FILE)
        return true
    else
        warn("âš ï¸ No se pudo guardar. Tu ejecutor necesita writefile()")
        return false
    end
end

function SaveSystem.LoadFromFile(): {[string]: any}?
    local success, result = pcall(function()
        if readfile and isfile and isfile(SAVE_FILE) then
            local jsonData = readfile(SAVE_FILE)
            return HttpService:JSONDecode(jsonData)
        end
        if syn and syn.read_file then
            local jsonData = syn.read_file(SAVE_FILE)
            return HttpService:JSONDecode(jsonData)
        end
        return nil
    end)
    
    if success and result then
        print("ğŸ“‚ Memoria cargada desde: " .. SAVE_FILE)
        return result
    else
        print("ğŸ“ No hay memoria guardada (primera vez)")
        return nil
    end
end

function SaveSystem.DeleteFile()
    if delfile and isfile and isfile(SAVE_FILE) then
        delfile(SAVE_FILE)
        print("ğŸ—‘ï¸ Memoria borrada")
        return true
    end
    return false
end

